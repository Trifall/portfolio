{
	"name": "vite-react-ts-tw-shadcn-template",
	"version": "1.0.1",
	"type": "module",
	"description": "A template with Vite, React, TypeScript, TailwindCSS, and ShadCN UI.",
	"author": {
		"name": "Jerren Trifan",
		"email": "Jerren@trifall.com"
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"serve": "vite preview",
		"lint:fix": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" --cache --fix --ignore-path ./.gitignore",
		"lint:format": "prettier --ignore-path .gitignore --log-level warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
		"lint": "yarn lint:fix && yarn lint:format ",
		"pipeline": "yarn && yarn type-check && yarn lint && yarn build",
		"type-check": "tsc --noEmit",
		"prepare": "husky install"
	},
	"dependencies": {
		"@pixi/filter-kawase-blur": "3.2.0",
		"@radix-ui/react-slot": "^1.0.2",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.0",
		"debounce": "^2.0.0",
		"framer-motion": "^11.0.8",
		"hsl-to-hex": "^1.0.0",
		"lucide-react": "^0.344.0",
		"pixi.js": "5.3.12",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-next-parallax": "^0.1.0",
		"react-scroll": "^1.9.0",
		"simplex-noise": "3.0.1",
		"tailwind-merge": "^2.2.1",
		"tailwindcss-animate": "^1.0.7"
	},
	"devDependencies": {
		"@types/node": "^20.11.24",
		"@types/react": "^18.2.61",
		"@types/react-dom": "^18.2.19",
		"@types/react-scroll": "^1.8.10",
		"@typescript-eslint/eslint-plugin": "^7.1.0",
		"@typescript-eslint/parser": "^7.1.0",
		"@vitejs/plugin-react-swc": "^3.6.0",
		"autoprefixer": "^10.4.18",
		"depcheck": "^1.4.7",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-react": "^7.34.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"postcss": "^8.4.35",
		"prettier": "^3.2.5",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"tailwindcss": "^3.4.1",
		"typescript": "^5.3.3",
		"vite": "^5.1.4"
	},
	"license": "MIT",
	"packageManager": "yarn@3.6.3"
}
